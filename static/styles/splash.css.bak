html { 
    -webkit-font-smoothing: antialiased; 
}

body { 
    height: 100%;
}

body {  
    margin: 0px;
    padding: 0;
    background: #C1D6E2;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 14px;
}

h1, h2, h3 {
    margin: 0;
    font-weight: normal;
}

h1 { 
    font-size: 28px;
    color: #333333; 
}

h2 { 
    font-size: 28px; 
    color: #639CBF;
}

p, ul, ol {
    margin-top: 0;
    text-align: justify;
}

a { 
    text-decoration: none; 
    color: #639CBF;
}

/*a:hover { 
    text-decoration: none; 
}*/

a img {
    border: none;
}

.clear { 
    clear: both; 
}

#wrapper {
    width: 100%;
    float: left;
}

#banner {
    position: absolute;
    z-index: -1;
    height: 150px;
    width: 1026px;
    background: white url(../images/img01.jpg) repeat-x left top;
}

/*
Header
*/
    
#splash_header {
    margin: auto;
    margin-top: 5px;
    height: 150px;
    background: white url(../images/img01.jpg) repeat-x left top;
}

#splash_header #introQuote {
    width: 493px;
    padding-top: 10px;
    padding-left: 20px;
}

#splash_header #introQuote {
    font-weight: bold;
}

#splash_header #introQuote h1{
    font-size: 130%;
}

#splash_header #flashSS {
    float:   right;
    height:  232px;  
    width:   232px;  
    padding: 0;  
    margin:  0;
}

#splash_header #flashSS img {  
    padding: 15px;  
    border:  1px solid #ccc;  
    background-color: #eee;  
    width:  200px; 
    height: 200px; 
    top:  0; 
    left: 0 
} 

#splash_header a {
    float: right;
    text-decoration: none;
}

#splash_header a:hover .box{
    width: 127px;
    height: 122px;
    margin-top: 7px;
    margin-right: 8px;
    padding: 0px;
    background: #FFFFFF;
    border: 1px solid #8D9AB0;
    /float: left;
    
    -webkit-box-shadow: 5px 5px 10px #064169;        
    -moz-box-shadow: 5px 5px 10px #064169;
    box-shadow: 5px 5px 10px #064169;
    -moz-border-radius-topleft: 20px;
    -moz-border-radius-topright: 20px;
    -moz-border-radius-bottomright: 20px;
    -moz-border-radius-bottomleft: 20px;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    border-bottom-left-radius: 20px;
}

#splash_header .box {
    width: 127px;
    height: 122px;
    margin-top: 7px;
    margin-right: 8px;
    padding: 0px;
    background: #FFFFFF;
    border: 1px solid #8D9AB0;
    /float: left;
    position:relative;
    
    -webkit-box-shadow: 3px 3px 8px #386699;        
    -moz-box-shadow: 3px 3px 8px #386699;
    box-shadow: 3px 3px 8px #386699;
    -moz-border-radius-topleft: 20px;
    -moz-border-radius-topright: 20px;
    -moz-border-radius-bottomright: 20px;
    -moz-border-radius-bottomleft: 20px;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    border-bottom-left-radius: 20px;
}


#splash_header .box h2{
    margin-top: 5px;
    text-align: center;
    font-size: 100px;
    line-height: 100%;
}

#splash_header .box p{
    padding-right: 10px; 
}

/*
Feature
*/

#feature {
    width: 986px;
    margin: 0 auto;
    padding: 5px 20px 0px 20px;
    background: #C1D6E2;
    
    /-moz-border-radius-topleft: 20px;
    /-moz-border-radius-topright: 20px;
    /-moz-border-radius-bottomright: 20px;
    /-moz-border-radius-bottomleft: 20px;
    /border-top-left-radius: 20px;
    /border-top-right-radius: 20px;
    /border-bottom-right-radius: 20px;
    /border-bottom-left-radius: 20px;
}

#feature a {
    text-decoration: none;
}

#feature a:hover .box{
    width: 200px;
    height: 170px;
    margin: 10px;
    padding: 10px;
    background: #FFFFFF;
    border: 1px solid #8D9AB0;
    float: left;
    
    -webkit-box-shadow: 5px 5px 10px #064169;        
    -moz-box-shadow: 5px 5px 10px #064169;
    box-shadow: 5px 5px 10px #064169;
    -moz-border-radius-topleft: 20px;
    -moz-border-radius-topright: 20px;
    -moz-border-radius-bottomright: 20px;
    -moz-border-radius-bottomleft: 20px;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    border-bottom-left-radius: 20px;
}

#feature .box {
    width: 200px;
    height: 170px;
    margin: 10px;
    padding: 10px;
    background: #FFFFFF;
    border: 1px solid #8D9AB0;
    float: left;
    position:relative;
    
    -webkit-box-shadow: 3px 3px 8px #386699;        
    -moz-box-shadow: 3px 3px 8px #386699;
    box-shadow: 3px 3px 8px #386699;
    -moz-border-radius-topleft: 20px;
    -moz-border-radius-topright: 20px;
    -moz-border-radius-bottomright: 20px;
    -moz-border-radius-bottomleft: 20px;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    border-bottom-left-radius: 20px;
}


#feature .box h2{
    margin-top: 0px;
    text-align: center;
}

#feature .box p{
    padding-right: 10px; 
}

h2.letter {
    font-family: 'Didact Gothic', arial, serif;
    font-size: 170px;
    margin: 0px;
    padding: 0px;
}

p.subscript {
    font-size: 20px; 
    position: absolute; 
    bottom: 5px;
    right: 5px;
    color: #aaa;
}

#feature .box p {
    text-align: center;
    padding: 0px;
    margin-top: 30px;
}

.subPage #feature {
    margin-top: 20px;
    -moz-border-radius-topleft: 20px;
    -moz-border-radius-topright: 20px;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
}


