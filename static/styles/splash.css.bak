#fatfooter {
   width: 900px;
   margin: 0 auto;
   color: #757E82;
}

#fatfooter p {
   margin: 0;
   text-align: center;
   font-size: 11px;
}

#fatfooter a {
   text-decoration: underline;
   color: #757E82;
}

#fatfooter a:hover {
   text-decoration: none;
   color: #F90542;
